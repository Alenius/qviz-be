{
  "name": "qviz-be",
  "version": "1.0.0",
  "description": "backend for simple quiz website",
  "main": "dist/app.js",
  "scripts": {
    "start": "nodemon dist/app.js",
    "start:dev": "nodemon -r dotenv/config --watch src/** --watch types --ext ts,json --ignore src/**/*.spec.ts --exec ts-node --files types/express/index.d.ts src/app.ts dotenv_config_path=./config/.env",
    "test": "test",
    "build": "tsc",
    "postinstall": "tsc"
  },
  "keywords": [
    "quiz",
    "nodejs"
  ],
  "author": "adam alenius",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fuzzyset.js": "^1.0.6",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.7",
    "pg": "^8.5.1",
    "ramda": "^0.27.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/fuzzyset.js": "^0.0.2",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/pg": "^7.14.9",
    "@types/ramda": "^0.27.36",
    "eslint": "^7.18.0"
  },
  "engines": {
    "node": "12.18.3"
  }
}
